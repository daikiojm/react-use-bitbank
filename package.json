    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "@swc/jest"
    }
  },
  "prettier": {
    "printWidth": 140,
