  "repository": "daikiojm/react-use-bitbank",
  "homepage": "https://github.com/daikiojm/react-use-bitbank#readme",
  "license": "MIT",
  "keywords": [
    "react",
    "react-hooks",
    "bitbank",
    "bitbank.cc"
